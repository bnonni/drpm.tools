{
  "name": "dpm",
  "version": "0.0.1",
  "type": "module",
  "description": "Decentralized Package Manager (DPM) leverages decentralized identifiers (DIDs) to reference software packages stored on the DWeb as a record in the publisher's Decentralized Web Node (DWN)",
  "directories": {
    "doc": "docs"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "bin": "./dist/esm/cli/dpm.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf dist coverage tests/compiled",
    "build:tests": "npm run build && tsc -p tests/tsconfig.json",
    "test:register-hooks": "npm run build:tests; node --import ./dist/esm/src/register.js ./tests/compiled/tests/register-hooks.spec.js",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "rimraf dist/cjs && tsc -p tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > ./dist/cjs/package.json",
    "build:esm": "rimraf dist/esm dist/types && tsc -p tsconfig.esm.json",
    "postinstall": "sh ./scripts/postinstall.sh",
    "prepare": "npm run build && chmod +x ./dist/esm/cli/dpm.js"
  },
  "homepage": "https://dpm.software",
  "bugs": {
    "url": "https://github.com/bnonni/dpm/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bnonni/dpm.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Bryan Nonni",
      "url": "https://github.com/bnonni"
    },
    {
      "name": "Daniel Buchner",
      "url": "https://github.com/csuwildcat"
    }
  ],
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "decentralized",
    "decentralized-applications",
    "decentralized-identity",
    "decentralized-web",
    "web5",
    "decentralized package manager",
    "dpm",
    "package manager",
    "dwn",
    "dweb node",
    "decentralized web node",
    "dwn protocol"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@web5/agent": "^0.7.4",
    "@web5/api": "^0.10.0",
    "@web5/dids": "^1.1.4",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "tar": "^7.4.3"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.5.4",
    "@types/npm": "^7.19.3",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "c8": "^10.1.2",
    "chai": "^5.1.1",
    "esbuild": "^0.23.1",
    "eslint-plugin-mocha": "^10.5.0",
    "global-dirs": "^3.0.1",
    "globals": "^15.9.0",
    "marked": "^14.1.2",
    "mocha": "^10.7.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0"
  }
}
